  "name": "zoOthello",
  "version": "1.0.0",
  "description": "A react(no CRA)/Express/mongoDB Othello game",
  "main": "server.js",
  "scripts": {
    "server": "nodemon --exec babel-node lib/server.js",
    "webpack": "webpack --mode development -w",
    "dev": "webpack --mode development && nodemon --exec babel-node lib/server.js",
    "test:client": "jest react/src --env=jsdom",
    "test:server": "env MONGO_CONFIG_URL='mongodb://root:rootpassword@localhost:27017' nyc mocha lib/test/api --recursive --exit",
    "test:all": "npm run test:server && npm run test:client",
    "build": "webpack --mode production",
    "lint": "eslint '**/*.js' --no-error-on-unmatched-pattern"
